<div class="container">
<h1> Add New TVReview </h1>
<%= form_for :tvreview, url: tv_reviews_path do |f| %>
<div class="form-group">
	<%= f.label :author %>
	<%= f.text_field :author %>
</div>
<div class="form-group">
	<%= f.label :rating %>
	<%= f.number_field :rating %>
</div>
<div class="form-group">
	<%= f.label :content %>
	<%= f.text_area :content %>
</div>
<div class="form-group">
	<%= f.label :date %>
	<%= f.date_field :date %>
</div>
<div class="form-group">
	<%= f.submit "Submit",class:'btn btn-primary' %>
	<%=link_to "Cancel" ,tv_reviews_path, class:'btn btn-default' %>
</div>
<% end %>
</div>