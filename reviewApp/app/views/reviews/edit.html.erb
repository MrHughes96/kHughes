<div class="container">
<h1> Edit Review </h1>
<%= form_for @review do |f| %>
<div class="form-group">
	<%= f.label :author %>
	<%= f.text_field :author %>
</div>
<div class="form-group">
	<%= f.label :rating %>
	<%= f.number_field :rating %>
</div>
<div class="form-group">
	<%= f.label :content %>
	<%= f.text_area :content %>
</div>
<div class="form-group">
	<%= f.label :date %>
	<%= f.date_field :date %>
</div>
<div class="form-group">
	<%= f.submit %>
</div>
<% end %>
</div>